{
  "expo": {
    "name": "Shaafi Hospital",
    "slug": "shaafi-hospital",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E3191"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kkimani.shaafihospital"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher.png",
        "backgroundColor": "#fff"
      },
      "package": "com.kkimani.shaafihospital"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-shaafi-transparent.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ed48e064-23b3-4f90-b069-69a23d777971"
      }
    },
    "owner": "kkimani"
  }
}
