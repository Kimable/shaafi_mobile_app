{
  "expo": {
    "name": "Shaafi Hospital",
    "slug": "shaafi-hospital",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "targetSdkVersion": 34,
    "splash": {
      "image": "./assets/images/logo-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a551"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kkimani.shaafihospital",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to upload images",
        "NSPhotoLibraryAddUsageDescription": "This app uses the photo library to upload images"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "package": "com.kkimani.shaafihospital",
      "versionCode": 11
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-shaafi-transparent.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ed48e064-23b3-4f90-b069-69a23d777971"
      }
    },
    "owner": "kkimani"
  }
}
